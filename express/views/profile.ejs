<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>
        User
        <%= name %>
    </title>
</head>

<body>
    <h1>Name
        <%= name %>
    </h1>
    <h2>Email:
        <%= data.email %>
    </h2>
    <h2>Age:
        <%= data.age %>
    </h2>
    <h3>
        Skills:
        <ul>
            <% data.skills.forEach(function(skill)   { %>

                <li>
                    <%=skill%>
                </li>
                <% }) %>

        </ul>
    </h3>
</body>

</html>